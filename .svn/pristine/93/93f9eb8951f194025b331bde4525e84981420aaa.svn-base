@media only screen and (min-width: 321px) and (max-width: 767px) {
    /* -----------------
	    GENERAL STYLES 
	------------------- */
    body {
        background-image: url('../img/bg_body_top-mobile.jpg');
    }
    .page {
        background-image: url('../img/bg_top-row-mobile.png');
    }
    .wrapper {
        width: auto;
        overflow: hidden !important;
        padding: 0 30px;
    }
    .desktop {
        display: none;
    }
    .mobile {
        display: block;
		clear:both;
    }
    /* ------------------
	    HEADER STYLES 
	------------------- */
    .header {
        height: 170px;
        overflow: hidden;
    }
    /* ------------------
	    HEADER TOP ROW 
	-------------------- */
    .header .top-row {
        height: 170px;
    }
    .header .top-row .logo {
        margin: 25px 0 24px;
        float: none;
        display: block;
    }
    .header .top-row .slogan {
        float: none;
        font-size: 20px;
    }
    .header .top-row h3 {
        font-size: 20px;
        letter-spacing: -1px;
        line-height: 31px;
        text-align: center;
    }
    .header .top-row h3 sup {
        vertical-align: top;
        font-size: 11px;
        top: 5px;
    }
    /* ---------------
	    BOTTOM ROW 
	--------------- */
    .bottom-row .cta {
        top: -62px;
        padding-left:5%;
        padding-right:5%;
    }
    .bottom-row .cta img {
        margin-bottom: 15px;
    }
	.bottom-row .apply{
	padding-top:10px;
	}
    .bottom-row .apply a {
        background: url('../img/cta_apply-now_center_1.png') repeat-x center center;
        font-size: 30px;
        height: 85px;
        line-height: 72px;
        display: inline-block;
        width: 93%;
        position: relative;
        margin-left: 11px;
    }
    .bottom-row .apply a:before {
        content:'';
        display: block;
        background: url('../img/cta_apply-now_left_1.png') no-repeat left bottom;
        height: 85px;
        width: 22px;
        position: absolute;
        left: -22px;
    }
    .bottom-row .apply a:after {
        content:'';
        display: inline-block;
        background: url('../img/cta_apply-now_right_1.png') no-repeat right bottom;
        height: 85px;
        width: 22px;
        right: -22px;
        position: absolute;
    }
    .bottom-row .apply a span {
        background: url('../img/cta_apply-now_arrow.png') no-repeat center center;
        display: inline-block;
        width: 20px;
        height: 20px;
    }
    .bottom-row .left {
        width: 100%;
        margin-top: -66px;
        padding-left: 0;
    }
    .bottom-row .headline h1 {
        font-size: 24px;
        line-height: 30px;
    }
    .bottom-row .headline h1 .one {
        font-size: 24px;
		line-height:25px;
        font-family: 'interstatebold';
    }
    .bottom-row .headline h1 .two {
   		line-height: 30px;
    }
    .bottom-row .headline h1 .two sup {
		vertical-align: top;
		top: 8px;
		font-size: 18px;
	}
    .bottom-row .headline h1 .three {
        font-size: 24px;
        line-height: 30px;
        position: relative;
        /*top: -18px;*/
    }
    .bottom-row .headline h1 br {
        display: none;
    }
    .bottom-row .headline h1 br.mobile {
        display: block;
    }
    .bottom-row .description {
        width: 100%;
        margin: 0;
        word-spacing: -1px;
        letter-spacing: -1px;
        /*padding-top: 10px;*/
    }
    .bottom-row .description p {
        font-size: 24px;
        line-height: 30px;
        font-family: 'interstatelight';
    }
    .bottom-row .description p .one {
        line-height: 27px;
    }
    .bottom-row .description p.small {
        font-size: 24px;
        line-height: 30px;
        word-spacing: 2px;
    }
    .bottom-row .description p span {
        font-size: 24px;
    }
    .bottom-row .headline h1 .four {
        font-size: 24px;
    }
    .bottom-row .description br {
        display: none;
    }
    .bottom-row .description p br.mobile {
        display: none;
    }
    .bottom-row .apply a {
        color:#fff;
        font-family:Arial;
        font-weight:bold;
        text-align:center;
        text-decoration:none;
    }
    /* ---------------
	    POINTS ROW 
	--------------- */
    .points-row {
        margin-top: 55px;
        border: 1px solid #cccccc;
        width: 100%;
    }
    .points-row .new {
        width: 100%;
        border: none;
        float: none;
        height: auto;
    }
    .points-row .new ul, .points-row .new li, .points-row .new .image, .points-row .new .image img {
        width: 99.1%;
    }
    .points-row .new .image span {
        font-size: 65px;
        line-height: 200px;
        width: 100%;
    }
    .points-row .new .description {
        float: none;
        width: 100%;
        margin: 220px 0 40px 10px;
        overflow: hidden;
    }
    .points-row .new .description img {
        margin: 26px 0 0 11px;
        float: left;
    }
    .points-row .new .description p {
        float: left;
        font-size: 25px;
        width: 220px;
        margin-left: 12px;
        margin-top: 5px;
    }
    .points-row .always {
        float: none;
        margin-top: 10px;
        padding: 8px 8px 17px 0;
        width: 100%;
    }
    .points-row .always .banner {
        left: -19px;
    }
    .points-row .always .description {
        float: none;
        width: 100%;
        margin: 50px 0 10px 10px;
        overflow: hidden;
    }
    .points-row .always .description img {
        margin: 20px 0 0 11px;
        float: left;
    }
    .points-row .always .description p {
        float: left;
        font-size: 25px;
        width: 185px;
        margin-left: 18px;
    }
    .points-row .always .description p br {
        display: none;
    }
    /* -------------
	    FEATURES 
	------------- */
    .features {
        margin-bottom: 24px;
    }
    .features .list-left, .features .list-right {
        float: none;
        width: auto;
        margin: 60px 0 0;
    }
    .features .list-right {
        margin-top: 0px !important;
    }
    .features .list-left ul li, .features .list-right ul li {
        margin-bottom: 38px;
    }
    .features .list-left ul li p, .features .list-right ul li p {
        font-size: 25px;
        line-height: 30px;
    }
    .features .cta {
        background: url('../img/cta_apply-now_center_1.png') repeat-x center center;
        font-size: 30px;
        height: 85px;
        line-height: 72px;
        display: inline-block;
        width: 93%;
        position: relative;
        margin-left: 11px;
    }
    .phone-info {
        font-size:25px;
        float:none;
        margin-left:0;
    }

    .phone-info span {
        white-space: nowrap;
    }
    .features .cta:before {
        content:'';
        display: block;
        background: url('../img/cta_apply-now_left_1.png') no-repeat left bottom;
        height: 85px;
        width: 22px;
        position: absolute;
        left: -22px;
    }
    .features .cta:after {
        content:'';
        display: inline-block;
        background: url('../img/cta_apply-now_right_1.png') no-repeat right bottom;
        height: 85px;
        width: 22px;
        right: -22px;
        position: absolute;
    }
    .features .cta span {
        background: url('../img/cta_apply-now_arrow.png') no-repeat center center;
        display: inline-block;
        width: 20px;
        height: 20px;
    }
    /* -----------
	    FOOTER 
	------------- */
    .footer .legal_box {
        display: none !important;
        height: 0px !important;
    }
    .footer .nav-small ul {
        margin: 0;
        padding: 0;
        list-style: none;
        margin-bottom: 30px;
    }
    .footer .nav-small ul li {
        margin-bottom: 5px;
    }
    .footer .nav-small ul li a {
        display: block;
        width: 100%;
        color: #ffffff;
        font-family: Arial;
        font-size: 25px;
        background: #cccccc;
        text-align: center;
        text-decoration: none;
        padding: 5px 0;
    }
    /* -----------
	    MODAL 
	------------- */
    #modal {
        padding-bottom: 30px !important;
        height: 210px;
    }
    #modal .questions {
        height: 170px !important;
    }
    .fancybox-wrap {
        width: 100% !important;
        height: 260px !important;
        position: absolute !important;
        left: 0 !important;
        opacity: 1 !important;
        overflow: scroll !important;
        padding-bottom: 20px !important;
        -webkit-overflow-scrolling: touch !important;
        margin-top: -95px !important;
    }
    .fancybox-skin {
        padding: 10px !important;
        padding-bottom: 20px !important;
        height: 220px !important;
    }
    .fancybox-inner {
        overflow: scroll !important;
        width: 100% !important;
        height: 210px !important;
        -webkit-overflow-scrolling: touch !important;
    }
}
